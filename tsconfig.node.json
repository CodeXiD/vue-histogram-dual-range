{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,

    "outDir": "dist",
    "declaration": true,
    "exclude": ["src/App.vue", "src/main.ts"]
  },
  "include": ["vite.config.ts"]
}
